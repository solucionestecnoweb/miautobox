<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- Add you code here -->
        <record model="ir.ui.view" id="id_almacenes_tree">
            <field name="name">id.almacenes.tree</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="before">
                    <field name="id" string="ID interno" readonly="1"/>
                </xpath>
            </field>
        </record>
        
        <record model="ir.ui.view" id="id_almacenes_form">
            <field name="name">id.almacenes.form</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="arch" type="xml">
               
                <xpath expr="//field[@name='code']" position="before">
                    <field name="id" string="ID interno" readonly="1"/>
                    
                </xpath>
                
            </field>
        </record>

      

    </data>
</odoo>