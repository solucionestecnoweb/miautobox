<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- Add you code here -->
        <record model="ir.ui.view" id="id_tipo_tree">
            <field name="name">id.tipo.tree</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_picking_type_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="before">
                    <field name="id" string="ID interno" readonly="1"/>
                </xpath>
            </field>
        </record>
        
        <record model="ir.ui.view" id="id_categoria_form">
            <field name="name">id.categoria.form</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_picking_type_form"/>
            <field name="arch" type="xml">
               
                <xpath expr="//field[@name='name']" position="before">
                    <field name="id" string="ID interno" readonly="1"/>
                    
                </xpath>
                
            </field>
        </record>

      

    </data>
</odoo>