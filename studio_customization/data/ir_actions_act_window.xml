<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="contacts.action_contacts" model="ir.actions.act_window" context="{'studio': True}">
    <field name="binding_model_id" eval="False"/>
    <field name="binding_type">action</field>
    <field name="binding_view_types">list,form</field>
    <field name="context">{'default_is_company': True}</field>
    <field name="domain" eval="False"/>
    <field name="filter" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="help"><![CDATA[<p class="o_view_nocontent_smiling_face">
            Clic para agregar un contacto en su directorio.
          </p><p>
            Odoo te ayuda a llevar el manejo de todas tus actividades relacionadas a un contacto.
          </p>
        ]]></field>
    <field name="limit">80</field>
    <field name="name">Contactos</field>
    <field name="res_model">res.partner</field>
    <field name="search_view_id" ref="base.view_res_partner_filter"/>
    <field name="target">current</field>
    <field name="type">ir.actions.act_window</field>
    <field name="usage" eval="False"/>
    <field name="view_id" eval="False"/>
    <field name="view_mode">tree,kanban,form,map,activity</field>
  </record>
  <record id="stock.product_template_action_product" model="ir.actions.act_window" context="{'studio': True}">
    <field name="binding_model_id" eval="False"/>
    <field name="binding_type">action</field>
    <field name="binding_view_types">list,form</field>
    <field name="context">{"search_default_consumable": 1, 'default_type': 'product'}</field>
    <field name="domain" eval="False"/>
    <field name="filter" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="help"><![CDATA[<p class="o_view_nocontent_smiling_face">
                No se ha encontrado ningún producto. ¡Creemos uno!
            </p><p>
                Track your stock quantities by creating storable products.
            </p>
        ]]></field>
    <field name="limit">80</field>
    <field name="name">Productos</field>
    <field name="res_model">product.template</field>
    <field name="search_view_id" ref="stock.product_template_search_form_view_stock"/>
    <field name="target">current</field>
    <field name="type">ir.actions.act_window</field>
    <field name="usage" eval="False"/>
    <field name="view_id" eval="False"/>
    <field name="view_mode">kanban,tree,form</field>
  </record>
